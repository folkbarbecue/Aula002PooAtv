package br.com.aula002;
import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
        Scanner tec = new Scanner(System.in);
 int valor1;
 int valor2;
		 System.out.println("Insira o valor 1");
		 valor1 = tec.nextInt();
		 System.out.println("Insira o valor 2");
		 valor2 = tec.nextInt();
		 if (valor1==valor2) {
			System.out.println("O valor 1 é Igual ao valor 2");
		}
		 if (valor1!=valor2) {
			System.out.println("O valor 1 é diferente do valor 2");
		}
		 if (valor1>valor2) {
			System.out.println("O valor 1 é maior que o valor 2");
		}
		 if (valor1<valor2) {
			System.out.println("O valor 1 é menor que o valor 2");
		}
		 if (valor1>=valor2) {
			System.out.println("O valor 1 é maior ou igual ao valor 2");
		}
		 if (valor1<=valor2) {
			System.out.println("O valor 1 é menor ou igual ao valor 2");
		}
}
}
